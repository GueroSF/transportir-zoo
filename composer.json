{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "8.0.*",
        "symfony/flex": "^1.12",
        "symfony/console": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    }
}
